graph TD
    subgraph CMD ["Command Bar Cmd K"]
        Input[Search Input: Create...]
        
        subgraph Results ["Results List"]
            R1[Create New Prompt Action]
            R2[Create New Collection Action]
            R3[Go to Settings Navigation]
            R4[Toggle Dark Mode Action]
        end
        
        Footer[Footer: Use Arrows to navigate, Enter to select]
    end
    
    %% Premium Styling
    classDef container fill:#1f2937,stroke:#374151,color:#f9fafb,stroke-width:1px;
    classDef input fill:#374151,stroke:#6b7280,color:#ffffff,stroke-width:2px;
    classDef resultActive fill:#3b82f6,stroke:none,color:#ffffff;
    classDef result fill:none,stroke:none,color:#d1d5db;
    
    class CMD container;
    class Input input;
    class R1 resultActive;
    class R2,R3,R4 result;
